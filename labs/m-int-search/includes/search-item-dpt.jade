each val, index in dpt
  label.col.s12.search-item(data-accordion)
    a.flight-detail-trigger.right(href="#" data-control) Detail ▾
    .row(style="padding:0;margin:0;margin-bottom:0.5em;")
      //- airlines name
      //- span.left= dpt[index].airlines
      span(style="color:#434343;") Round-trip Price
      strong(id="dpt-plussign-"+ [index] style="color:rgb(249,109,1);font-size:1.2em;")   
      strong.currency.left(id="dpt-price-"+ [index] style="color:rgb(249,109,1);font-size:1.2em;")= dpt[index].price
      span(style="color:rgb(249,109,1);") /pax
    .row(style="padding:0;margin:0;")
      .col.s6(style="padding-left:0")
        strong= dpt[index].time_out + " – " + dpt[index].time_arvl
        unless !dpt[index].time_tomorrow
          strong  (+1D)
        br
        span(style="color:#8f8f8f")= dpt[index].airport_route_out + " ⇄ " + dpt[index].airport_route_arvl
      .col.s6.right-align(style="padding-right:0")
        span=dpt[index].time_duration
        br
        if dpt[index].transit > 0
          span(style="color:#8f8f8f;")=dpt[index].transit + " transit(s)"
        else
          span(style="color:#8f8f8f;") direct flight
    hr(style="width:100%;margin:0.5em auto;border-bottom:1px dashed #e5e5e5;")  
    .row.valign-wrapper(style="padding:0;margin:0;")
      .col.s5(style="padding-left:0;position:relative;")
        //- span(style="color:#434343;") Round-trip Price
      .col.s7.right-align(style="padding-right:0;position:relative;")
        unless dpt[index].price_before == 0
          span.currency(style="color:#b4b4b4; text-decoration:line-through;")= dpt[index].price_before
          br
        //- used to the place for prices
        //- strong(id="dpt-plussign-"+ [index] style="color:rgb(249,109,1);font-size:1.2em;")   
        //- strong.currency(id="dpt-price-"+ [index] style="color:rgb(249,109,1);font-size:1.2em;")= dpt[index].price
        
    input(type="radio" name="dpt" value=dpt[index].price data-route= dpt[index].airport_route_out+" – "+dpt[index].airport_route_arvl data-time= dpt[index].time_out+" – "+dpt[index].time_arvl data-duration=dpt[index].time_duration)
    //- flight details
    .row.detail(data-content)
      hr(style="width:100%;margin:0.5em auto;border-bottom:1px solid #dadada;")
      p.center-align Inclusive of airline Fee & Taxes
      hr(style="width:100%;margin:0.5em auto;border-bottom:1px dashed #e5e5e5;")
      //- detail tabs here
      .row
        .col.s12
          ul.tabs
            li.tab.col.s6.center-align
              a.detail-tab.active(href= "#dpt-flight-detail-" + [index] style="color:#1ba0e2;") Flight
            li.tab.col.s6.center-align
              a.detail-tab(href="#dpt-facility-detail-" + [index] style="color:#1ba0e2;") Facilities
            //- li.tab.col.s4.center-align
              a.detail-tab(href="#price-detail-" + [index] style="color:#1ba0e2;") Price
      //- detail tabs end

      hr(style="width:100%;margin:0.5em auto;border-bottom:1px dashed #e5e5e5;")
      div(id="dpt-flight-detail-"+ [index] class=".col.s12")
        .row(style="margin-bottom:16px;")
          .col.s12 
            h5(style="font-size:1.3em;")= dpt[index].flight_code    
        .row
          .col.s3.right-align
            strong= dpt[index].time_out
            br
            span(style="color:#8f8f8f;")= dpt[index].date_out + " " + dpt[index].month_out
          .col.s9
            strong=dpt[index].city_route_out +"("+ dpt[index].airport_route_out +")"
            br
            span(style="color:#8f8f8f;")=dpt[index].long_airport_route_out
        .row
          .col.s3
            span &emsp;
          .col.s9
            span(style="color:#8f8f8f;") Duration: 
              span= dpt[index].time_duration
        .row
          .col.s3.right-align
            strong= dpt[index].time_arvl
            br
            span(style="color:#8f8f8f;")= dpt[index].date_arvl + " " + dpt[index].month_arvl
          .col.s9
            strong=dpt[index].city_route_arvl +"("+ dpt[index].airport_route_arvl +")"
            br
            span(style="color:#8f8f8f;")=dpt[index].long_airport_route_arvl 
      div(id="dpt-facility-detail-"+ [index] class=".col.s12")
        .row
          .col.s12
            span= dpt[index].psc
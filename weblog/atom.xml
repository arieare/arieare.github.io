---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="https://www.w3.org/2005/Atom">

	<title>Arie's Web Log</title>
	<link href="https://arieare.github.io/weblog/atom.xml" rel="self"/>
	<link href="https://arieare.github.io/weblog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>https://arieare.github.io/weblog</id>
	<author>
		<name>Arie</name>
		<email>arie.a.re@gmail.com</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="https://arieare.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>https://arieare.github.io{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
